<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.qwk.blindBox.mapper.BulletsMapper">
    
    <resultMap type="Bullets" id="BulletsResult">
        <result property="id"    column="id"    />
        <result property="name"    column="name"    />
        <result property="speed"    column="speed"    />
        <result property="placeId"    column="place_id"    />
        <result property="consume"    column="consume"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectBulletsVo">
        select id, name, speed, place_id, consume, create_by, create_time, update_by, update_time, remark from bullets
    </sql>

    <select id="selectBulletsList" parameterType="Bullets" resultMap="BulletsResult">
        <include refid="selectBulletsVo"/>
        <where>  
            <if test="name != null  and name != ''"> and name like concat('%', #{name}, '%')</if>
            <if test="speed != null "> and speed = #{speed}</if>
            <if test="placeId != null "> and place_id = #{placeId}</if>
            <if test="consume != null "> and consume = #{consume}</if>
            <if test="createBy != null  and createBy != ''"> and create_by like concat('%', #{createBy}, '%')</if>
            <if test="params.beginCreateTime != null and params.beginCreateTime != '' and params.endCreateTime != null and params.endCreateTime != ''"> and create_time between #{params.beginCreateTime} and #{params.endCreateTime}</if>
            <if test="updateBy != null  and updateBy != ''"> and update_by like concat('%', #{updateBy}, '%')</if>
            <if test="params.beginUpdateTime != null and params.beginUpdateTime != '' and params.endUpdateTime != null and params.endUpdateTime != ''"> and update_time between #{params.beginUpdateTime} and #{params.endUpdateTime}</if>
        </where>
    </select>
    
    <select id="selectBulletsById" parameterType="Long" resultMap="BulletsResult">
        <include refid="selectBulletsVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertBullets" parameterType="Bullets" useGeneratedKeys="true" keyProperty="id">
        insert into bullets
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="name != null">name,</if>
            <if test="speed != null">speed,</if>
            <if test="placeId != null">place_id,</if>
            <if test="consume != null">consume,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="name != null">#{name},</if>
            <if test="speed != null">#{speed},</if>
            <if test="placeId != null">#{placeId},</if>
            <if test="consume != null">#{consume},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateBullets" parameterType="Bullets">
        update bullets
        <trim prefix="SET" suffixOverrides=",">
            <if test="name != null">name = #{name},</if>
            <if test="speed != null">speed = #{speed},</if>
            <if test="placeId != null">place_id = #{placeId},</if>
            <if test="consume != null">consume = #{consume},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteBulletsById" parameterType="Long">
        delete from bullets where id = #{id}
    </delete>

    <delete id="deleteBulletsByIds" parameterType="String">
        delete from bullets where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

    <select id="getAllBullets" resultMap="BulletsResult">
        <include refid="selectBulletsVo"/>
    </select>

</mapper>