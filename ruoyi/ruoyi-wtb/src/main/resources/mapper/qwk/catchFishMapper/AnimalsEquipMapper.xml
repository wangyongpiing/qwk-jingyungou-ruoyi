<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.qwk.blindBox.mapper.AnimalsEquipMapper">
    
    <resultMap type="AnimalsEquip" id="AnimalsEquipResult">
        <result property="id"    column="id"    />
        <result property="name"    column="name"    />
        <result property="position"    column="position"    />
        <result property="rank"    column="rank"    />
        <result property="attack"    column="attack"    />
        <result property="life"    column="life"    />
        <result property="lasting"    column="lasting"    />
        <result property="skill"    column="skill"    />
        <result property="icon"    column="icon"    />
        <result property="overt"    column="overt"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectAnimalsEquipVo">
        select id, name, position, rank, attack, life, lasting, skill, icon, overt, create_by, create_time, update_by, update_time, remark from animals_equip
    </sql>

    <select id="selectAnimalsEquipList" parameterType="AnimalsEquip" resultMap="AnimalsEquipResult">
        <include refid="selectAnimalsEquipVo"/>
        <where>  
            <if test="name != null  and name != ''"> and name like concat('%', #{name}, '%')</if>
            <if test="position != null  and position != ''"> and position = #{position}</if>
            <if test="rank != null "> and rank = #{rank}</if>
            <if test="attack != null  and attack != ''"> and attack = #{attack}</if>
            <if test="life != null  and life != ''"> and life = #{life}</if>
            <if test="lasting != null  and lasting != ''"> and lasting = #{lasting}</if>
            <if test="skill != null  and skill != ''"> and skill = #{skill}</if>
            <if test="icon != null  and icon != ''"> and icon = #{icon}</if>
            <if test="overt != null  and overt != ''"> and overt = #{overt}</if>
            <if test="createBy != null  and createBy != ''"> and create_by like concat('%', #{createBy}, '%')</if>
            <if test="params.beginCreateTime != null and params.beginCreateTime != '' and params.endCreateTime != null and params.endCreateTime != ''"> and create_time between #{params.beginCreateTime} and #{params.endCreateTime}</if>
            <if test="updateBy != null  and updateBy != ''"> and update_by like concat('%', #{updateBy}, '%')</if>
            <if test="params.beginUpdateTime != null and params.beginUpdateTime != '' and params.endUpdateTime != null and params.endUpdateTime != ''"> and update_time between #{params.beginUpdateTime} and #{params.endUpdateTime}</if>
        </where>
    </select>
    
    <select id="selectAnimalsEquipById" parameterType="Long" resultMap="AnimalsEquipResult">
        <include refid="selectAnimalsEquipVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertAnimalsEquip" parameterType="AnimalsEquip" useGeneratedKeys="true" keyProperty="id">
        insert into animals_equip
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="name != null">name,</if>
            <if test="position != null">position,</if>
            <if test="rank != null">rank,</if>
            <if test="attack != null">attack,</if>
            <if test="life != null">life,</if>
            <if test="lasting != null">lasting,</if>
            <if test="skill != null">skill,</if>
            <if test="icon != null">icon,</if>
            <if test="overt != null">overt,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="name != null">#{name},</if>
            <if test="position != null">#{position},</if>
            <if test="rank != null">#{rank},</if>
            <if test="attack != null">#{attack},</if>
            <if test="life != null">#{life},</if>
            <if test="lasting != null">#{lasting},</if>
            <if test="skill != null">#{skill},</if>
            <if test="icon != null">#{icon},</if>
            <if test="overt != null">#{overt},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateAnimalsEquip" parameterType="AnimalsEquip">
        update animals_equip
        <trim prefix="SET" suffixOverrides=",">
            <if test="name != null">name = #{name},</if>
            <if test="position != null">position = #{position},</if>
            <if test="rank != null">rank = #{rank},</if>
            <if test="attack != null">attack = #{attack},</if>
            <if test="life != null">life = #{life},</if>
            <if test="lasting != null">lasting = #{lasting},</if>
            <if test="skill != null">skill = #{skill},</if>
            <if test="icon != null">icon = #{icon},</if>
            <if test="overt != null">overt = #{overt},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteAnimalsEquipById" parameterType="Long">
        delete from animals_equip where id = #{id}
    </delete>

    <delete id="deleteAnimalsEquipByIds" parameterType="String">
        delete from animals_equip where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>