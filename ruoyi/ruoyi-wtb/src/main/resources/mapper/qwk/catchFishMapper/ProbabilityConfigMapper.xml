<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.qwk.blindBox.mapper.ProbabilityConfigMapper">

    <resultMap type="ProbabilityConfig" id="ProbabilityConfigResult">
        <result property="id"    column="id"    />
        <result property="fMId"    column="f_m_id"    />
        <result property="fieldId"    column="field_id"    />
        <result property="fn"    column="Fn"    />
        <result property="oi"    column="Oi"    />
        <result property="ui"    column="Ui"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectProbabilityConfigVo">
        select id, f_m_id, field_id, Fn, Oi, Ui, create_by, create_time, update_by, update_time, remark from probability_config
    </sql>

    <select id="selectProbabilityConfigList" parameterType="ProbabilityConfig" resultMap="ProbabilityConfigResult">
        <include refid="selectProbabilityConfigVo"/>
        <where>
            <if test="fMId != null "> and f_m_id = #{fMId}</if>
            <if test="fieldId != null "> and field_id = #{fieldId}</if>
            <if test="fn != null "> and Fn = #{fn}</if>
            <if test="oi != null "> and Oi = #{oi}</if>
            <if test="ui != null "> and Ui = #{ui}</if>
            <if test="createBy != null  and createBy != ''"> and create_by like concat('%', #{createBy}, '%')</if>
            <if test="params.beginCreateTime != null and params.beginCreateTime != '' and params.endCreateTime != null and params.endCreateTime != ''"> and create_time between #{params.beginCreateTime} and #{params.endCreateTime}</if>
        </where>
    </select>

    <select id="selectProbabilityConfigById" parameterType="Long" resultMap="ProbabilityConfigResult">
        <include refid="selectProbabilityConfigVo"/>
        where id = #{id}
    </select>

    <insert id="insertProbabilityConfig" parameterType="ProbabilityConfig" useGeneratedKeys="true" keyProperty="id">
        insert into probability_config
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="fMId != null">f_m_id,</if>
            <if test="fieldId != null">field_id,</if>
            <if test="fn != null">Fn,</if>
            <if test="oi != null">Oi,</if>
            <if test="ui != null">Ui,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="fMId != null">#{fMId},</if>
            <if test="fieldId != null">#{fieldId},</if>
            <if test="fn != null">#{fn},</if>
            <if test="oi != null">#{oi},</if>
            <if test="ui != null">#{ui},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
        </trim>
    </insert>

    <update id="updateProbabilityConfig" parameterType="ProbabilityConfig">
        update probability_config
        <trim prefix="SET" suffixOverrides=",">
            <if test="fMId != null">f_m_id = #{fMId},</if>
            <if test="fieldId != null">field_id = #{fieldId},</if>
            <if test="fn != null">Fn = #{fn},</if>
            <if test="oi != null">Oi = #{oi},</if>
            <if test="ui != null">Ui = #{ui},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteProbabilityConfigById" parameterType="Long">
        delete from probability_config where id = #{id}
    </delete>

    <delete id="deleteProbabilityConfigByIds" parameterType="String">
        delete from probability_config where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
    
    <select id="selectByFieldId" resultMap="ProbabilityConfigResult">
        <include refid="selectProbabilityConfigVo"/>
        where f_m_id = #{fMId} and field_id = #{fieldId} limit 1
    </select>
    
</mapper>