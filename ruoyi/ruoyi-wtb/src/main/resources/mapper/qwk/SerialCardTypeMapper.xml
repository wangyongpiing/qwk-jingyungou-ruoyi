<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.qwk.mapper.SerialCardTypeMapper">
    
    <resultMap type="SerialCardType" id="SerialCardTypeResult">
        <result property="id"    column="id"    />
        <result property="type"    column="type"    />
        <result property="number"    column="number"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectSerialCardTypeVo">
        select id, type, number, create_by, create_time, update_by, update_time from serial_card_type
    </sql>

    <select id="selectSerialCardTypeList" parameterType="SerialCardType" resultMap="SerialCardTypeResult">
        <include refid="selectSerialCardTypeVo"/>
        <where>  
            <if test="type != null  and type != ''"> and type = #{type}</if>
            <if test="number != null  and number != ''"> and number = #{number}</if>
        </where>
    </select>
    
    <select id="selectSerialCardTypeById" parameterType="Long" resultMap="SerialCardTypeResult">
        <include refid="selectSerialCardTypeVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertSerialCardType" parameterType="SerialCardType" useGeneratedKeys="true" keyProperty="id">
        insert into serial_card_type
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="type != null">type,</if>
            <if test="number != null">number,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="type != null">#{type},</if>
            <if test="number != null">#{number},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateSerialCardType" parameterType="SerialCardType">
        update serial_card_type
        <trim prefix="SET" suffixOverrides=",">
            <if test="type != null">type = #{type},</if>
            <if test="number != null">number = #{number},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteSerialCardTypeById" parameterType="Long">
        delete from serial_card_type where id = #{id}
    </delete>

    <delete id="deleteSerialCardTypeByIds" parameterType="String">
        delete from serial_card_type where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>