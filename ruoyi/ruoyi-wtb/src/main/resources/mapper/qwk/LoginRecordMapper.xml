<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.qwk.mapper.LoginRecordMapper">

    <resultMap type="LoginRecord" id="LoginRecordResult">
        <result property="id"    column="id"    />
        <result property="ip"    column="ip"    />
        <result property="subIp"    column="sub_ip"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectLoginRecordVo">
        select id, ip, create_by, create_time, update_by, update_time from login_record
    </sql>

    <select id="selectLoginRecordList" parameterType="LoginRecord" resultMap="LoginRecordResult">
        <include refid="selectLoginRecordVo"/>
        <where>
            <if test="ip != null  and ip != ''"> and ip = #{ip}</if>
        </where>
    </select>

    <select id="selectLoginRecordById" parameterType="Long" resultMap="LoginRecordResult">
        <include refid="selectLoginRecordVo"/>
        where id = #{id}
    </select>

    <insert id="insertLoginRecord" parameterType="LoginRecord" useGeneratedKeys="true" keyProperty="id">
        insert into login_record
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="ip != null">ip,</if>
            <if test="subIp != null">sub_ip,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="ip != null">#{ip},</if>
            <if test="subIp != null">#{subIp},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateLoginRecord" parameterType="LoginRecord">
        update login_record
        <trim prefix="SET" suffixOverrides=",">
            <if test="ip != null">ip = #{ip},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteLoginRecordById" parameterType="Long">
        delete from login_record where id = #{id}
    </delete>

    <delete id="deleteLoginRecordByIds" parameterType="String">
        delete from login_record where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>
